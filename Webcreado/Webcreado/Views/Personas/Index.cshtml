@{
}
<h1>Persona</h1>
<p>
    <a asp-action="Crear">Ingresar Persona</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Cedula
            </th>
            <th>
                Nombre
            </th>
            <th>
                Apellido
            </th>
            <th>
                Direccion
            </th>
            <th>
                Genero
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
         {
        <tr>
            <td>@item.Cedula</td>
            <td>@item.Nombre</td>
            <td>@item.Apellido</td>
            <td>@item.Direccion</td>
            <td>@item.Genero</td>
        </tr>
         }
    </tbody>
</table>
